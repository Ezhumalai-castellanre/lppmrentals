{
  "paths": {
    "/s3-upload": {
      "name": "/s3-upload",
      "lambdaFunction": "s3-upload",
      "permissions": {
        "setting": "open"
      },
      "maxPayloadSize": "50MB",
      "cors": {
        "allowOrigin": "*",
        "allowHeaders": ["Content-Type", "X-Amz-Date", "Authorization", "X-Api-Key", "X-Amz-Security-Token"],
        "allowMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
      }
    },
    "/webhook-proxy": {
